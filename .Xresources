! -- COLOR -- !
Xterm*termName: xterm-256color
URxvt*termName: rxvt-256color

Xft.lcdfilter: lcddefault
!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

! -- Fonts -- !
URxvt*font: xft:Hack:size=8:hinting=True;antialias:True
URxvt*boldFont: xft:Hack:bold:size=8;hinting=True:antialias:True
